{
  "version": 2,
  "builds": [
    {
      "src": "idp.js", // Replace 'server.js' with the name of your main server file
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "idp.js" // Replace 'server.js' with the name of your main server file
    }
  ]
}
